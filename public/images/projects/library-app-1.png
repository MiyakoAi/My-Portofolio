<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#1e293b"/>
  <rect x="40" y="40" width="720" height="520" fill="#334155" stroke="#64748b" stroke-width="2"/>
  
  <!-- Header -->
  <rect x="60" y="60" width="680" height="60" fill="#475569"/>
  <text x="400" y="95" font-family="monospace" font-size="24" fill="#e2e8f0" text-anchor="middle">Library Management System</text>
  
  <!-- Sidebar -->
  <rect x="60" y="140" width="200" height="400" fill="#3f3f46"/>

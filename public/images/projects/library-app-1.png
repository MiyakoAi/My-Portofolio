  <!-- Main Content -->
  <rect x="280" y="140" width="480" height="400" fill="#27272a"/>
  <text x="520" y="170" font-family="monospace" font-size="18" fill="#f4f4f5" text-anchor="middle">Book Statistics</text>
  

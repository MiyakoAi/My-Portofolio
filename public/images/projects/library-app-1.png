  <text x="690" y="260" font-family="monospace" font-size="24" fill="#fff" text-anchor="middle">258</text>
  
  <!-- Table -->
  <rect x="300" y="310" width="440" height="200" fill="#18181b" stroke="#3f3f46"/>
  <text x="520" y="335" font-family="monospace" font-size="14" fill="#f4f4f5" text-anchor="middle">Recent Activities</text>
  
  <!-- Table rows -->
  <rect x="310" y="350" width="420" height="25" fill="#27272a"/>
  <text x="320" y="367" font-family="monospace" font-size="10" fill="#d4d4d8">Book returned: "JavaScript Guide" by John Doe</text>
  
  <rect x="310" y="380" width="420" height="25" fill="#1f1f23"/>
  <text x="320" y="397" font-family="monospace" font-size="10" fill="#d4d4d8">New member registered: Jane Smith</text>
  
  <rect x="310" y="410" width="420" height="25" fill="#27272a"/>
  <text x="320" y="427" font-family="monospace" font-size="10" fill="#d4d4d8">Book borrowed: "React Patterns" by Mike Wilson</text>
